{
    "name": "ingor/ingor",
    "description": "Quick and easy to customize admin panel for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Yang",
            "email": "yangchenshin77@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "admin",
        "customize",
        "generator"
    ],
    "homepage": "https://github.com/ingorlabs/ingor",
    "require": {
        "php": "^7.4|^8.0",
        "additionapps/flexible-presenter": "^2.1",
        "facade/ignition-contracts": "^1.0",
        "illuminate/contracts": "^8.40",
        "inertiajs/inertia-laravel": "^0.4",
        "laravel/framework": "^8.40",
        "laravel/ui": "^3.0",
        "lorisleiva/laravel-actions": "^2.1",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ingor\\": "src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ingor\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ingor\\IngorServiceProvider"
            ],
            "aliases": {
                "Ingor": "Ingor\\Ingor"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
